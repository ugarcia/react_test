  "browser": {
    "jquery": "./node_modules/jquery/dist/jquery.min.js",
    "bootstrap": "./node_modules/bootstrap/dist/js/bootstrap.min.js",
    "underscore": "./node_modules/backbone/node_modules/underscore/underscore.js",
    "backbone": "./node_modules/backbone/backbone.js",
    "react": "./node_modules/react/dist/react-with-addons.min.js"
  },
  "browserify-shim": {
    "jquery": "$",
    "underscore": "_",
    "bootstrap": {
      "depends": [
        "jquery:jQuery"
      ]
    },
    "backbone": {
      "exports": "Backbone",
      "depends": [
        "underscore:underscore",
        "jquery:$"
      ]
    },
    "react": {}
  },
  "browserify": {
    "transform": [
      "babelify",
      "browserify-shim"
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [],
    "ignore": [
      "node_modules",
      "app/components",
      "app/app.js",
      "build",
      "gulp",
      "gulpfile.js"
    ]
  }
